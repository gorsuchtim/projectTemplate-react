{
  "name": "webpack_template",
  "version": "1.0.0",
  "description": "A template build structure for a webpack build",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run format && webpack --config webpack.production.js",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config webpack.development.js --open",
    "format": "npm run lint && npm run prettier",
    "lint": "eslint \"src/**/*.js\"",
    "prettier": "prettier --write \"src/**/*.{html,htm,css,scss,json,js,jsx}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorsuchtim/webpack_template.git"
  },
  "keywords": [],
  "author": "Tim Gorsuch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gorsuchtim/webpack_template/issues"
  },
  "homepage": "https://github.com/gorsuchtim/webpack_template#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-shorthand-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "3.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass": "^1.23.0-module.beta.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {}
}
