{
  "name": "webpack_template",
  "version": "1.0.0",
  "description": "A template folder structure for a webpack build",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run format && npm run webpack -- --env.mode production",
    "clean": "rimraf dist",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot",
    "format": "npm run prettier",
    "prettier": "prettier --write \"src/**/*.{htm, css, scss, json, js, jsx}\" --write",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorsuchtim/webpack_template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gorsuchtim/webpack_template/issues"
  },
  "homepage": "https://github.com/gorsuchtim/webpack_template#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6"
  }
}
