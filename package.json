{
  "name": "webpack_template",
  "version": "1.0.0",
  "description": "A template folder structure for a webpack build",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run prettier && npm run webpack -- --env.mode production",
    "clean": "rimraf dist",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot",
    "prettier": "prettier --write \"src/**/*.{htm,css,scss,json,js,jsx}\" --write",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorsuchtim/webpack_template.git"
  },
  "keywords": [],
